{
  "name": "ts-for-gjs",
  "version": "1.0.0",
  "description": "Typescript bindings for GJS",
  "main": "dist/bin.js",
  "scripts": {
    "test": "ava test.ts",
    "build": "ts-node fuse.ts",
    "watch": "tsc --watch",
    "prepack": "ts-node fuse.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammydre/ts-for-gjs.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sammydre/ts-for-gjs/issues"
  },
  "homepage": "https://github.com/sammydre/ts-for-gjs#readme",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.66",
    "@types/node": "^12.0.2",
    "@types/xml2js": "0.4.4",
    "ava": "^1.4.1",
    "commander": "^2.9.0",
    "events": "^3.0.0",
    "prettier": "^1.17.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "uglify-js": "^3.5.14"
  },
  "dependencies": {
    "fuse-box": "^3.7.1",
    "lodash": "^4.17.11",
    "xml2js": "^0.4.17"
  }
}
